{
  "scripts": {
    "format": "prettier --write '{.,__tests__,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "precommit": "tsc && lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "test": "jest",
    "tsc": "tsc -p .",
    "xcode": "open ios/$npm_package_name.xcodeproj"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["prettier --write", "tslint --fix", "git add"]
  },
  "dependencies": {
    "@types/react-navigation": "latest",
    "hoist-non-react-statics": "latest",
    "react": "latest",
    "react-native": "latest",
    "react-native-gesture-handler": "latest",
    "react-navigation": "latest",
    "react-navigation-tabs": "latest",
    "react-redux": "latest",
    "redux": "latest",
    "redux-persist": "latest"
  }
}
